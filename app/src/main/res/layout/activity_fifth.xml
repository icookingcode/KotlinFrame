<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".FifthActivity">

    <com.guc.kframe.widget.toolbar.TitleLayout
        android:id="@+id/titleLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:leftType="finish"
        app:title="@string/forth_page" />

    <TextView
        android:id="@+id/tvShow"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btnGetAssets"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="获取Assets文件" />

    <Button
        android:id="@+id/btnGetAssetsObject"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="获取Assets中对象" />

    <Button
        android:id="@+id/btnFindLocation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="开始定位" />

    <ExpandableListView
        android:id="@+id/expandableListView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@null" />


</LinearLayout>